set terminal qt
set key outside
set key font 'Arial,10'
set tics font 'Arial,12'
set grid
set title 'Solitaire Simulation' font 'Arial,20'
set xlabel 'Simulation Number' font 'Arial,16'
set ylabel 'Number of Games to 60 PerCent' font 'Arial,16'
set style line 1 linecolor rgb '#0060ad' linetype 1 linewidth 2 pointtype 7 pointsize 1.5
plot 'gnuplotData' using 1:2 title 'Simulated Data' with points linestyle 1
